{
  "presets": [
    ["@stellar-apps/react", {"removePropTypes":  false}]
  ],
  "env": {
    "cjs": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "useBuiltIns": false,
              "targets": {"browsers": ">5% in US"}
            },
            "runtime": false,
            "closureElimination": false
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "useBuiltIns": false,
              "modules": false,
              "targets": {"browsers": ">5% in US"}
            },
            "runtime": false,
            "closureElimination":  false
          }
        ]
      ]
    }
  }
}
